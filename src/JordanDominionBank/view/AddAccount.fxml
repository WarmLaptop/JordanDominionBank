<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.collections.FXCollections?>
<DialogPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="JordanDominionBank.view.AddAccount">
    <headerText>
        Add Account
    </headerText>
    <content>
        <GridPane>
            <Label text="account Type: " GridPane.rowIndex="0" GridPane.columnIndex="0"></Label>
            <ComboBox fx:id="accountType" GridPane.rowIndex="0" GridPane.columnIndex="1">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="Saving"></String>
                        <String fx:value="Chequing"></String>
                    </FXCollections>

                </items>

            </ComboBox>
        </GridPane>
    </content>


</DialogPane>
